{
	"title": "Contact",
	"model": {
		"builder": [
			{
				"type": "intro",
				"title": "Ready to talk?",
				"content": "Give us a ring, or fill out the form below and we'll get in touch with you as soon as possible."
			},
			{
				"type": "contactForm",
				"action": "https://formspree.io/f/mbjpeppq",
				"schema": [
					{
						"label": "First name",
						"name": "firstName",
						"validation": "required"
					},
					{
						"label": "Last name",
						"name": "lastName",
						"validation": "required"
					},
					{
						"label": "Your email",
						"name": "email",
						"validation": "bail|required|email"
					},
					{
						"label": "Phone number",
						"name": "phoneNumber",
						"type": "tel",
						"inputmode": "numeric",
						"pattern": "[0-9]*",
						"validation": "number"
					},
					{
						"label": "Company",
						"name": "company"
					}
				]
			}
		]
	}
}
